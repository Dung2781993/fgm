<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>FGM - Our Communities </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet" type="text/css" />*@


    @* Content-box fixes as per http://docs.telerik.com/kendo-ui/third-party/using-kendo-with-twitter-bootstrap article  *@


    <link href="@Url.Content("~/Content/box-sizing-fixes.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="~/js/jquery.js"></script>
    <script src="~/js/Password.js"></script>



    <!--Styesheet of template-->

    <link href="@Url.Content("~/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/image.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/color/default.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/font/Reklame.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.4.0/dist/leaflet.css"
          integrity="sha512-puBpdR0798OZvTTbP4A8Ix/l+A4dHDD0DGqYW6RQ+9jxkRFclaxxQb/SJAWZfWAkuyeQUytO7+7N4QKrDh+drA=="
          crossorigin="" />
    <link href="@Url.Content("~/css/map.css")" rel="stylesheet" type="text/css" />

    <script src="https://unpkg.com/leaflet@1.4.0/dist/leaflet.js"
            integrity="sha512-QVftwZFqvtRNi0ZyCtsznlKSWOStnDORoefr1enyq5mVL4tmKB3S/EnC3rRJcxCPavG10IcrVGSmPh6Qw5lwrg=="
            crossorigin=""></script>
    <script src="https://d3js.org/d3.v3.min.js"></script>


    <!---------------Pie Chart --------------->
    <link href="https://cdn.anychart.com/releases/v8/css/anychart-ui.min.css" rel="stylesheet" type="text/css">
    <link href="https://cdn.anychart.com/releases/v8/fonts/css/anychart-font.css" rel="stylesheet" type="text/css">
    <script src="https://cdn.anychart.com/releases/v8/js/anychart-base.min.js"></script>
    <script src="https://cdn.anychart.com/releases/v8/js/anychart-ui.min.js"></script>




</head>
<body>

    <div class="navbar-wrapper">
        <div class="navbar navbar-fixed-top" style="background-color:#FFF;">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                </a>
                <h4 class="brand"><a href="@Url.Action("Index", "Home")" style="font-family:Reklame Script Regular DEMO; font-size: 50px;">Feel Good Mate</a></h4>
                <div class="pull-right nav-collapse collapse">
                    <ul id="menu-main" class="nav navbar-nav">
                        <li><a title="home" href="@Url.Action("Index", "Home")">Home</a></li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Regional Cities <span class="caret"></span></a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="@Url.Action("Geelong", "City")">Geelong</a></li>
                                <li><a href="@Url.Action("Ballarat", "City")">Ballarat</a></li>
                                <li><a href="@Url.Action("Bendigo", "City")">Bendigo</a></li>
                                <li><a href="@Url.Action("Warrnambool", "City")">Warrnambool</a></li>
                                <li><a href="@Url.Action("Latrobe", "City")">Latrobe</a></li>
                            </ul>
                        </li>
                        <li><a title="comparison" href="@Url.Action("Comparison", "City")">Comparison</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    @RenderBody()


    <footer>
        <div class="container">
            <div class="row">
                <div class="span4 offset1">
                    <div>
                        <p>We live with <strong>creativity</strong></p>
                        <p>
                            Coming from different corners of the world our team believes in cultural acceptance and the power of diversity.
                            Never letting the enthusiasm die, we strive for perfection.
                        </p>
                    </div>
                </div>
                <div class="span6">
                    <img src="/Images/logo-white.png" alt="" style="padding-left:20px; width:50%;" />
                </div>
            </div>
        </div>
        <!-- ./container -->
    </footer>
    <a href="#" class="scrollup"><i class="icon-angle-up icon-square icon-bgdark icon-2x"></i></a>

    @Scripts.Render("~/bundles/common")

</body>

</html>
